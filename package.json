{
  "name": "test-plug-a-h",
  "version": "0.0.1",
  "description": "Test vue plugin. Base on vue3.3 + ts + vite",
  "files": [
    "dist",
    "types"
  ],
  "main": "./dist/vgce.umd.cjs",
  "module": "./dist/vgce.js",
  "exports": {
    ".": {
      "import": "./dist/vgce.js",
      "require": "./dist/vgce.umd.cjs",
      "types": "./types/index.d.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "types": "types/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lib": "vue-tsc --noEmit --skipLibCheck && vite build --mode lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RickyHeaven/RickyBigProject.git"
  },
  "keywords": [
    "vue",
    "pinia",
    "vite",
    "ricky",
    "zhangqingcq"
  ],
  "author": "RickyHeaven",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RickyHeaven/RickyBigProject/issues"
  },
  "dependencies": {
    "pinia": "^2.1.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/tsconfig": "^0.4.0",
    "less": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vue-tsc": "^1.6.5"
  }
}
